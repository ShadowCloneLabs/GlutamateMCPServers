{
  "name": "@modelcontextprotocol/server-browser-tools",
  "version": "1.0.0",
  "description": "Model Context Protocol server for browser tools and screenshot capture",
  "license": "MIT",
  "author": "",
  "homepage": "https://github.com/ShadowCloneLabs/GlutamateMCPServers",
  "bugs": "https://github.com/ShadowCloneLabs/GlutamateMCPServers/issues",
  "repository": {
